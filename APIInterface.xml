						</Texture>
						<FontString parentKey="text" inherits="GameFontHighlightSmall" justifyH="LEFT" justifyV="MIDDLE">
							
							<Anchors>
								<Anchor point="TOPLEFT" x="25" y="0"/>
								<Anchor point="BOTTOMRIGHT" x="-25" y="0"/>
							</Anchors>
